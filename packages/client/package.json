{
  "name": "@app_sdk_internal/client",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "./build/index.cjs",
  "types": "./build/index.d.ts",
  "module": "./build/index.mjs",
  "browser": "./build/index.mjs",
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "concurrently -n=build:esm,build:commonjs,build:types \"npm run build:esm\" \"npm run build:commonjs\" \"npm run build:types\"",
    "build:esm": "NODE_ENV=development esbuild src/index.ts --outdir=./build --packages=external --bundle --format=esm --analyze=verbose --out-extension:.js=.mjs",
    "build:commonjs": "NODE_ENV=development esbuild src/index.ts --outdir=./build --packages=external --bundle --platform=node --out-extension:.js=.cjs --analyze=verbose",
    "build:types": "tsc --emitDeclarationOnly",
    "build:prod": "npm run clean && concurrently -n=build:prod:esm,build:prod:commonjs,build:types \"npm run build:prod:esm\" \"npm run build:prod:commonjs\" \"npm run build:types\"",
    "build:prod:esm": "NODE_ENV=production esbuild src/index.ts --outdir=./build --packages=external --bundle --format=esm --analyze=verbose --out-extension:.js=.mjs",
    "build:prod:commonjs": "NODE_ENV=production esbuild src/index.ts --outdir=./build --packages=external --bundle --platform=node --out-extension:.js=.cjs --analyze=verbose",
    "watch": "concurrently -n=watch:esm,watch:commonjs,watch:types \"npm run watch:esm\" \"npm run watch:commonjs\" \"npm run watch:types\"",
    "watch:esm": "NODE_ENV=development esbuild src/index.ts --outdir=./build --packages=external --bundle --format=esm --watch --analyze=verbose --out-extension:.js=.mjs",
    "watch:commonjs": "NODE_ENV=development esbuild src/index.ts --outdir=./build --packages=external --bundle --platform=node --watch --out-extension:.js=.cjs --analyze=verbose",
    "watch:types": "tsc --watch --emitDeclarationOnly --preserveWatchOutput"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "client-only": "^0.0.1",
    "esbuild": "^0.19.3",
    "jwt-decode": "^4.0.0",
    "openid-client": "^5.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.22",
    "concurrently": "^8.2.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "next": ">=13.4",
    "next-auth": "^4.23.1",
    "react": ">=18.0.0"
  }
}
