{
  "name": "@5minds/processcube_app_sdk",
  "version": "0.0.1",
  "description": "The SDK for ProcessCube Apps",
  "type": "module",
  "main": "server/index.js",
  "module": "server/index.js",
  "browser": "client/index.js",
  "types": "server/index.d.ts",
  "exports": {
    ".": {
      "types": "./server/index.d.ts",
      "default":"./server/index.js"
    },
    "./server": {
      "types": "./server/index.d.ts",
      "default":"./server/index.js"
    },
    "./client": {
      "types": {
        "default": "./client/index.d.ts"
      },
      "default": "./client/index.js"
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && tsc -p tsconfig.client.json",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/5minds/ProcessCube.App.SDK.git"
  },
  "files": [
    "client",
    "server"
  ],
  "keywords": [
    "SDK",
    "ProcessCube",
    "Next",
    "Nextjs",
    "React",
    "5Minds"
  ],
  "author": "Alexander Kasten",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/5minds/ProcessCube.App.SDK/issues",
    "email": "alexander.kasten@5minds.de"
  },
  "homepage": "https://github.com/5minds/ProcessCube.App.SDK#readme",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/react": "^18.2.13",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "next": ">=13.4",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@5minds/processcube_engine_client": "^4.3.1"
  }
}
