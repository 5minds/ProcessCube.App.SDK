{
  "name": "@5minds/processcube_app_sdk",
  "version": "5.1.0-alpha.1",
  "description": "The SDK for ProcessCube Apps",
  "type": "module",
  "main": "build/common/index.cjs",
  "module": "build/common/index.mjs",
  "browser": "build/client/index.mjs",
  "types": "build/common/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/common/index.d.ts",
      "require": "./build/common/index.cjs",
      "default": "./build/common/index.mjs"
    },
    "./server": {
      "types": "./build/server/index.d.ts",
      "require": "./build/server/index.cjs",
      "default": "./build/server/index.mjs"
    },
    "./client": {
      "types": "./build/client/index.d.ts",
      "require": "./build/client/index.cjs",
      "default": "./build/client/index.mjs"
    }
  },
  "scripts": {
    "clean": "rm -rf build",
    "prepare": "npm run build:prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "concurrently -n=build:code,build:types \"NODE_ENV=development node build.js\" \"npm run build:types\"",
    "build:types": "tsc -p tsconfig.common.json --emitDeclarationOnly && concurrently -n=build:types:server,build:types:client \"tsc -p tsconfig.server.json --emitDeclarationOnly\" \"tsc -p tsconfig.client.json --emitDeclarationOnly\"",
    "build:prod": "npm run clean && concurrently -n=build:prod:code,build:types \"NODE_ENV=production node build.js\" \"npm run build:types\"",
    "watch": "NODE_ENV=development tsc -p tsconfig.common.json --emitDeclarationOnly && concurrently --kill-others -n=esbuild,tsc:server,tsc:common,tsc:client \"node build.js --watch\" \"tsc -p tsconfig.server.json --watch --preserveWatchOutput --emitDeclarationOnly\" \"tsc -p tsconfig.common.json --watch --preserveWatchOutput --emitDeclarationOnly\" \"tsc -p tsconfig.client.json --watch --preserveWatchOutput --emitDeclarationOnly\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/5minds/ProcessCube.App.SDK.git"
  },
  "files": [
    "build/client",
    "build/common",
    "build/server"
  ],
  "keywords": [
    "SDK",
    "ProcessCube",
    "Next",
    "Nextjs",
    "React",
    "5Minds"
  ],
  "author": "Alexander Kasten",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/5minds/ProcessCube.App.SDK/issues",
    "email": "product@5minds.de"
  },
  "homepage": "https://processcube.io/docs/app-sdk",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.22",
    "concurrently": "^8.2.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "next": ">=14",
    "next-auth": "~4.24.6",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@5minds/processcube_engine_client": "2024-1",
    "@5minds/processcube_engine_sdk": "2024-1",
    "chokidar": "^3.5.3",
    "client-only": "^0.0.1",
    "esbuild": "^0.20.0",
    "jwt-decode": "^4.0.0",
    "only-server": "^0.0.1",
    "openid-client": "^5.5.0"
  }
}
